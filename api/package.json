{
  "name": "desafio-sirio",
  "version": "1.0.0",
  "description": "API de cadastro de usuarios com sistema de login baseado em JWT",
  "main": "index.js",
  "scripts": {
    "start": "node dist/infra/http/server.js",
    "dev": "tsnd --respawn --transpile-only --ignore-watch node_modules --exit-child --clear -r tsconfig-paths/register src/infra/http/server.ts",
    "test": "jest",
    "db:migrations": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:prepare": "npm run db:migrations && npm run db:seed",
    "build": "babel src --extensions \".ts\" --out-dir dist --copy-files"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.24.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.12",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "uuidv4": "^6.2.13"
  }
}
